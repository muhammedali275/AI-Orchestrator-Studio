# AIpanel Environment Configuration

# Server Settings
AIPANEL_PORT=8000
DEBUG=false

# TLS Settings
TLS_ENABLED=false
TLS_CERT_PATH=
TLS_KEY_PATH=

# Database Settings
DB_CONNECTION_STRING=sqlite:///./aipanel.db
# For PostgreSQL:
# DB_CONNECTION_STRING=postgresql://user:password@localhost:5432/aipanel

# LLM Settings
DEFAULT_AGENT_NAME=zain_agent
LLM_BASE_URL=
LLM_API_KEY=
LLM_DEFAULT_MODEL=
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=
LLM_TIMEOUT_SECONDS=60
LLM_MAX_RETRIES=3

# API Settings
API_PREFIX=/api
CORS_ORIGINS=http://localhost:3000

# Security Settings
API_KEY_HEADER=X-API-Key
JWT_SECRET_KEY=CHANGE_ME_IN_PRODUCTION
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=30

# Memory Settings
MEMORY_ENABLED=true
MEMORY_MAX_MESSAGES=50

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600

# Redis Settings (optional)
REDIS_HOST=
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Tool Settings
TOOLS_EXECUTION_TIMEOUT=30
TOOLS_MAX_CONCURRENT=5

# Orchestration Settings
ORCHESTRATION_MAX_ITERATIONS=10
ORCHESTRATION_TIMEOUT_SECONDS=300
