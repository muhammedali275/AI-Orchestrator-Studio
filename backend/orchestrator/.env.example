# exampleOne Orchestrator Studio - Environment Configuration Template
# Copy this file to .env and fill in your values
# The GUI will manage these values at runtime

# Application Settings
APP_NAME=exampleOne Orchestrator Studio
APP_VERSION=1.0.0
DEBUG=false
LOG_LEVEL=INFO

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/v1
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# LLM Configuration
LLM_BASE_URL=
LLM_DEFAULT_MODEL=
LLM_TIMEOUT_SECONDS=60
LLM_MAX_RETRIES=3
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=
LLM_API_KEY=

# External Agent Configuration
EXTERNAL_AGENT_BASE_URL=
EXTERNAL_AGENT_AUTH_TOKEN=
EXTERNAL_AGENT_TIMEOUT_SECONDS=30

# Data Source Configuration
DATASOURCE_BASE_URL=
DATASOURCE_AUTH_TOKEN=
DATASOURCE_TIMEOUT_SECONDS=30

# PostgreSQL Configuration
POSTGRES_HOST=
POSTGRES_PORT=5432
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DATABASE=
# Or use DSN directly:
# POSTGRES_DSN=postgresql://user:password@host:port/database

# Redis Configuration
REDIS_HOST=
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
# Or use URL directly:
# REDIS_URL=redis://:password@host:port/db
REDIS_TTL_SECONDS=3600

# Vector Database Configuration
VECTOR_DB_TYPE=chroma
VECTOR_DB_URL=
VECTOR_DB_API_KEY=
VECTOR_DB_COLLECTION=orchestrator_vectors
VECTOR_DB_DIMENSION=1536

# Memory Configuration
MEMORY_ENABLED=true
MEMORY_MAX_MESSAGES=50
MEMORY_SUMMARY_ENABLED=true

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600

# Tool Configuration
TOOLS_CONFIG_PATH=
TOOLS_EXECUTION_TIMEOUT=30
TOOLS_MAX_CONCURRENT=5

# Grounding Configuration
GROUNDING_ENABLED=true
GROUNDING_TOP_K=5
GROUNDING_SIMILARITY_THRESHOLD=0.7

# Orchestration Configuration
ORCHESTRATION_MAX_ITERATIONS=10
ORCHESTRATION_TIMEOUT_SECONDS=300

# Monitoring Configuration
MONITORING_ENABLED=true
MONITORING_EXPORT_INTERVAL=60

# Security Configuration
AUTH_ENABLED=false
AUTH_SECRET_KEY=
AUTH_ALGORITHM=HS256
AUTH_TOKEN_EXPIRE_MINUTES=30
